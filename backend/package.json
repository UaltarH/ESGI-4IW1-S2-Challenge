{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node --watch src/index.js",
    "dev:legacy": "nodemon -L src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "migrate-up": " sequelize-cli db:migrate --migrations-path ./src/sequelize/migrations --config ./src/config/sequelize_config.js",
    "migrate-down-all": "sequelize-cli db:migrate:undo:all",
    "migrate-down-last": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed-down": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.3",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "ts-jest": "^29.1.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "sequelize-cli": "^6.6.2"
  }
}
